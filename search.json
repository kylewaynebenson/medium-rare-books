---
layout: null
---
[
  {% for book in site.data.books %}
    {
      "title"    : "{{ book.title | escape }}",
      "category" : "{{ book.category }}",
      "tags"     : "{{ book.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}/books{{ book.title | datapage_url: dir}}",
      "date"     : "{{ book.date }}",
      "description"     : "{{ book.description }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
