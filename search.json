---
layout: null
---
[
  {% for book in site.data.books %}
    {
      "title"    : "{{ book.title | escape }}",
      "category" : "{{ book.category }}",
      "tags"     : "{{ book.tags | join: ', ' }}",
      "url"      : "{{ book.title | datapage_url: 'books' }}",
      "date"     : "{{ book.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
